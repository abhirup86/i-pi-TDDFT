<simulation>
   <initialize nbeads='24'>
      <file mode='pdb'> our_ref.pdb </file>
      <velocities mode='thermal' units='kelvin'> 34 </velocities>
   </initialize>
   <output prefix=''>
      <properties shape='(7)' filename='ph2-p24-17K-0bar-isotropic.out'>
         [ step, time{picosecond}, conserved{kelvin}, temperature{kelvin}, kinetic_cv{kelvin}, potential{kelvin}, pressure_cv{megapascal}, volume]
      </properties>
      <trajectory format='pdb' filename='ph2-p24-17K-0bar-isotropic.traj' stride='100' cell_units='angstrom'>positions{angstrom}</trajectory>
   </output>
   <total_steps>20000</total_steps>
   <prng><seed>31417</seed></prng>
   <forces>
      <socket mode="unix">
         <address>localhost</address>
      </socket>
   </forces>
   <ensemble mode='nvt'>
      <thermostat mode='pile_g'>
         <tau units='femtosecond'> 10 </tau>
      </thermostat>
      <timestep units='femtosecond'>1  </timestep>
      <temperature units='kelvin'>  1.70000000e+01</temperature>
      <pressure>0</pressure>
   </ensemble>
</simulation>
